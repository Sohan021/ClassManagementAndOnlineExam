
@model onlineexam.ViewModels.SemesterViewModel
@{
    ViewData["Title"] = "Semester";
}
<div class="row">
    <div class="col-md-3">
    </div>
    <div class="col-md-6">
        <form method="post" enctype="multipart/form-data" asp-controller="Account" asp-action="create" class="mt-3">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div>
                <h1 align="center">Create Semester</h1>
            </div>
            <div class="form-group ">
                <label asp-for="Name"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BatchId">Select Batch</label>
                <select asp-for="BatchId" asp-items="ViewBag.bId" class="form-control"></select>
                <span asp-validation-for="BatchId" class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-primary" align="center">Submit</button>
        </form>
    </div>
    <div class="col-md-3">
    </div>
</div>
